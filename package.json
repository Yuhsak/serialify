{
  "name": "serialify",
  "version": "0.4.4",
  "description": "Object serializer/deserializer for node.js written with TypeScript",
  "author": {
    "name": "Yuhsak Inoue",
    "email": "yuhsak.inoue@gmail.com",
    "url": "https://github.com/Yuhsak"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yuhsak/serialify.git"
  },
  "main": "lib/common/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "files": [
    "lib",
    "!lib/**/*.tsbuildinfo",
    "src",
    "tsconfig.json",
    "tsconfig.esm.json"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "test": "jest",
    "build": "npm run clean && tsc -b tsconfig.json tsconfig.esm.json",
    "build:watch": "npm run build -- --watch",
    "start": "npm run build:watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.1",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  }
}
